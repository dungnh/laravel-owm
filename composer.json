{
    "name": "coliving/laravel-owm",
    "description": "Wrapper for cmfcmf/openweathermap-php-api. This package fork from gmopx/laravel-owm as it's abandoned",
    "keywords": ["weather","clima","open weather map","laravel"],
    "require": {
        "php": ">=7.2.0",
        "cmfcmf/openweathermap-php-api": "^3.0",
        "php-http/discovery": "^1.13"
    },
    "require-dev": {
        "php-http/mock-client": "1.4.x-dev"
    },
    "license": "MIT",

    "authors": [
        {
            "name": "Dzung Nguyen",
            "email": "dungnh@gmail.com"
        }
    ],
    "minimum-stability": "dev",

    "autoload": {
        "psr-4": {
            "Gmopx\\LaravelOWM\\": "src/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Gmopx\\LaravelOWM\\LaravelOWMServiceProvider"
            ]
        }
    }
}
